spring.application.name=demo

spring.datasource.url=jdbc:mysql://localhost:3306/notes
spring.datasource.username=moad
spring.datasource.password=moadmoad

spring.jpa.hibernate.ddl-auto=update

spring.main.banner-mode=off
# Reduce logging level
logging.level.root=warn


# config to display SQL statement
# Log SQL statement
logging.level.org.hibernate.SQL=debug 
# Log values for SQL statement
logging.level.org.hibernate.orm.jdbc.bind=trace

# login form
spring.security.user.name=user
spring.security.user.password=moadmoad

